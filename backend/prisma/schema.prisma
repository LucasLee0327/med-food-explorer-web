generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("AIVEN_POSTGRES_PRISMA_URL")
}

model food {
  id  Int @id @default(autoincrement())
  name  String  @unique
  style String
  type  String
  price String
  arr_time  String
  address String  @default("尚未上傳地址QwQ")
  latitude  Float?
  longitude Float?
} 
